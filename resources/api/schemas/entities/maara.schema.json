{
  "$schema": "http://json-schema.org/draft-03/schema#",
  "id": "urn:harja",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "materiaali": {
      "id": "urn:harja/maara/materiaali",
      "enum": [
        "talvisuolaliuosNaCl",
        "talvisuolaliuosCaCl2",
        "erityisalueetNaCl",
        "erityisalueetNaClLiuos",
        "hiekoitushiekka",
        "kaliumformiaatti"
      ]
    },
    "yksikko": {
      "id": "urn:harja/yksikko",
      "required": true,
      "enum": [
        "t",
        "kg",
        "km",
        "m",
        "euro",
        "kpl"
      ]
    },
    "maara": {
      "id": "urn:harja/maara",
      "type": "number",
      "required": true
    }
  }
}