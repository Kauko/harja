  <xs:complexType name="CurveType">
    <xs:sequence>
      <xs:element ref="Segment" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="Segment">
    <xs:complexType>
      <xs:attribute name="start" type="projectDateTimeType" use="required"/>
      <xs:attribute name="finish" type="projectDateTimeType" use="required"/>
      <xs:attribute name="sum" type="xs:float" use="required"/>
      <!-- start date of curve segment -->
      <!-- finish date of curve segment -->
      <!-- sum (in hours) of segment -->
    </xs:complexType>
  </xs:element>
  <xs:element name="Baselines">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Baseline" type="BaselineType" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="BaselineType">
    <xs:sequence>
      <xs:element name="BaselineDetails" type="BaselineDetailType" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="revisionID" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="10"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="delete" type="xs:boolean"/>
