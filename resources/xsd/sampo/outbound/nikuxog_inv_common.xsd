          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="80"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="description">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="80"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="BaselineDetailType">
    <xs:annotation>
      <xs:documentation>
        The attributes do not necessarily reflect the content of the curves; Task and Project
        start/finish/duration are independent of the curve.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="UsageCurve" type="BaselineCurveType" minOccurs="0"/>
      <xs:element name="CostCurve" type="BaselineCurveType" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="start" type="projectDateTimeType"/>
    <xs:attribute name="finish" type="projectDateTimeType"/>
    <xs:attribute name="usageSum" type="xs:float"/>
    <xs:attribute name="costSum" type="xs:float"/>
    <xs:attribute name="duration" type="xs:float"/>
  </xs:complexType>
  <xs:complexType name="BaselineCurveType">
    <xs:sequence>
      <xs:element ref="Segment" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ExpressionText">
    <xs:simpleContent>
      <xs:extension base="xs:string"/>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="RuleType">
    <xs:sequence>
      <xs:element name="expression" type="ExpressionText"/>
      <xs:element name="condition" type="ExpressionText" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="isDefault" type="xs:boolean" use="required"/>
    <xs:attribute name="delete" type="xs:boolean"/>
  </xs:complexType>
  <xs:element name="estimateRules">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="rule" type="RuleType" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="SkillAssocsType">
    <xs:annotation>
      <xs:documentation xml:lang="en">
          The skill Association section.  List of skill Associations.

          isComplete - If the flag is true, then if there is any associations missing they are deleted.
                      Otherwise, we only insert and/or update
          complete - This flag has been introduced to remove the inconsistency in naming the complete flag across all files.
                     Eventually this flag will replace the usage of completed/isComplete flags that are currently being used for the deletion purpose.

      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="SkillAssoc" type="SkillAssocType" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="isComplete" type="xs:boolean" default="false"/>
    <xs:attribute name="complete" type="xs:boolean" default="false"/>
  </xs:complexType>
  <xs:complexType name="SkillAssocType">
    <xs:annotation>
      <xs:documentation xml:lang="en">
      The skills that the Resources associated with.

      skillCode - The code of the skill
      interestLevel - the resource's interest level in this skill
      proficiencyLevel - the resource's proficiency level in this skill
      weight - used, when searching, to produce a weighted average
      </xs:documentation>
    </xs:annotation>
    <xs:attribute name="skillCode" type="xs:string" use="required"/>
    <xs:attribute name="interestLevel" type="xs:string"/>
    <xs:attribute name="proficiencyLevel" type="xs:string"/>
    <xs:attribute name="weight" type="xs:integer" />
  </xs:complexType>
</xs:schema>
